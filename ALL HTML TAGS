                <h1 align="center">table with rowspan and colspan</h1>
                <center>
                <table border="2" align="center">
                    <caption>class time table</caption>
                    <tr><th colspan="6"> time table </th></tr>
                    <tr>
                        <th rowspan="6">hours</th>
                        <th>Mon</th>
                        <th>Tue</th>
                        <th>Wed</th>
                        <th>Thu</th>
                        <th>Fri</th>
                    </tr>
